    <!--
	###########################################################################
	#  location-code
	###########################################################################  -->
	<xs:element name="location-code">
		<xs:annotation>
			<xs:documentation>
                The location code for the containing entity.
			</xs:documentation>
		</xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="absolute" type="mrw:absolute-type" />
