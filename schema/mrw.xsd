    <!--
    ###########################################################################
    #  led-rollups
    ###########################################################################  -->
    <xs:element name="led-rollups">
        <xs:annotation>
            <xs:documentation>
                Defines a set of LED rollups.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="mrw:led-rollup" minOccurs="0" maxOccurs="unbounded" />
