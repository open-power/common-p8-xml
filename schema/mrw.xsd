            </xs:documentation>
        </xs:annotation>
    </xs:element>


    <!--
    ###########################################################################
    #  pcie-32b-mmio-window
    ###########################################################################  -->
    <xs:element name="pcie-32b-mmio-window" type="xs:nonNegativeInteger">
        <xs:annotation>
            <xs:documentation>
