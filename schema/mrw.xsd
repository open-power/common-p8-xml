                <xs:enumeration value="HHFL" />
                <xs:enumeration value="FHHL" />
                <xs:enumeration value="HHHL" />
                <xs:enumeration value="LP" />
                <xs:enumeration value="MEZZ" />
                <xs:enumeration value="N/A" />
            </xs:restriction>
        </xs:simpleType>
    </xs:element>

