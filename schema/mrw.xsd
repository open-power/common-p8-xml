            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <!--
    ###########################################################################
    #  pci-width
    ###########################################################################  -->
    <xs:element name="pci-width">
    <xs:annotation>
            <xs:documentation>
                PCI bus width
            </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:positiveInteger">
                <xs:enumeration value="1" />
                <xs:enumeration value="4" />
                <xs:enumeration value="8" />
                <xs:enumeration value="16" />
            </xs:restriction>
        </xs:simpleType>
    </xs:element>

	<!--
	###########################################################################
	#  pcies
	###########################################################################  -->
	<xs:element name="pcies">
