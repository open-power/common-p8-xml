	</xs:element>

	<!--
	###########################################################################
	#  pcie-endpoint-unit
	###########################################################################  -->
	<xs:element name="pcie-endpoint-unit">
		<xs:annotation>
			<xs:documentation>
                Defines a endpoint of a PCI-E unit.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="mrw:id" minOccurs="1" maxOccurs="1" />
